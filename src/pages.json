{
	"easycom": {
		"autoscan": true,
		"custom": {
			"fui-(.*)": "firstui-uni/firstui/fui-$1/fui-$1.vue"
		}
	},
	"pages": [ //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
		{
			"path": "pages/index/index",
			"style": {
				"navigationBarTitleText": ""
			}
		}
	    ,{
            "path" : "pages/TurnCamera/TurnCamera",
            "style" :                                                                                    
            {
                "navigationBarTitleText": "打开相机",
                "enablePullDownRefresh": false
            }
            
        }
        ,{
            "path" : "pages/CustomizePage/CustomizePage",
            "style" :                                                                                    
            {
                "navigationBarTitleText": "定制页面",
                "enablePullDownRefresh": false
            }
            
        }
        ,{
            "path" : "pages/OpenAlbum/OpenAlbum",
            "style" :                                                                                    
            {
                "navigationBarTitleText": "打开相册",
                "enablePullDownRefresh": false
            }
            
        }
        ,{
            "path" : "pages/SearchTargeting/SearchTargeting",
            "style" :                                                                                    
            {
                "navigationBarTitleText": "搜索定位",
                "enablePullDownRefresh": false
            }
            
        }
        ,{
            "path" : "pages/ShoppingCart/ShoppingCart",
            "style" :                                                                                    
            {
                "navigationBarTitleText": "购物车",
                "enablePullDownRefresh": false
            }
            
        }
        ,{
            "path" : "pages/PersonalCenter/PersonalCenter",
            "style" :                                                                                    
            {
                "navigationBarTitleText": "个人中心",
                "enablePullDownRefresh": false
            }
            
        }
    ],
	"globalStyle": {
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "uni-app",
		"navigationBarBackgroundColor": "#F8F8F8",
		"backgroundColor": "#F8F8F8"
	}
}
